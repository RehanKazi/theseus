{
  "manifest_version": 3,
  "name": "Theseus",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://discord.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}